ex58 is

  path := "path/to/file"

  lm : mutate is

  lm.instate_self ()->

    x := io.file.use (array u8) lm path io.file.mode.read (()->io.buffered.read_fully lm)

    match x
      content array => yak (String.from_bytes content) # logic in case of success
      err error => say err # error handling logic
