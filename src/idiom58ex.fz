ex58 is

  path := "path/to/file"

  lm : mutate is

  lm.instate_self ()->

    x := io.file.use (array u8) path io.file.mode.read ()->(io.buffered io.file.file_mutate).read_fully

    match x
      content array => yak (String.from content) # logic in case of success
      err error => say err # error handling logic
