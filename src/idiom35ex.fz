ex35 is

  A(val i32) is
  B(val bool) is
  C(val String) is

  compose(f A -> B, g B -> C) A -> C =>
    a -> g (f a)

  my_f A -> B := a -> B (a.val > 3)
  my_g B -> C := b -> C (if b.val then "T" else "F")

  fog := compose my_f my_g

  say (fog (A 10)).val
  say (fog (A  2)).val
  say (fog (A  0)).val
