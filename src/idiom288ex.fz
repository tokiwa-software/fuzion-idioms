    x := container.set_of_ordered [1,2,3,4,5]
    e := 0
    e2 := 1

  //running the example using 'contains'

    b := x.contains e
    if b=true
      say "the element e=$e exist in the set x=$x"
    else
      say "the element e=$e Does not exist in the set x=$x"

    say "-------------------------------------------------------"

  // running the example using '∈'

    b2 := e2 ∈ x
    if b2=true
      say "the element e=$e exist in the set x=$x"
    else
      say "the element e=$e Does not exist in the set x=$x"
