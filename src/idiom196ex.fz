ex196 is

  n i64 := 20
  m i64 := 13

  foo (E type : mutate, a container.Mutable_Array i64 E, i i64) ! E =>
    a[i] := 42

  mi : mutate is
  mi.instate_self ()->
    a := (mi.array i64).new n 0
    a.indices.for_each (i -> a[i] := i)

    say a

    ((i64 1)..m : 2).for_each (i -> foo a i)

    say a
