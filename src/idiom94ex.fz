ex94 =>

  x := "a string"
  y String := ref : String
                utf8 => x.utf8

  say (type_of x)
  say (type_of y)

  # or

  say (x.dynamic_type)
  say (y.dynamic_type)

  # For value instances `x`, this is equal to `type_of x`, but for `x`
  # with a  `ref` type `x.dynamic_type` gives the actual runtime type,
  # while `type_of x` results in the static compile-time type.

